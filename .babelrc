{
  "presets": [
    "env",
    "stage-2",
    "react"
  ],
  "plugins": [
    "syntax-trailing-function-commas",
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-runtime"
  ],
  "env": {
    "production": {
      "plugins": [ "transform-react-constant-elements" ]
    }
  },
  "comments": false,
  "sourceMaps": true
}
